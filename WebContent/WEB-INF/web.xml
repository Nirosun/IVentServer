<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

	<servlet>
	  <display-name>iVent Web Service</display-name>
	  <servlet-name>iVentServlet</servlet-name>
	  <servlet-class>servlet.IVentServlet</servlet-class>
	</servlet>
	 
	<servlet-mapping>
	  <servlet-name>iVentServlet</servlet-name>
	  <url-pattern>/ivent/*</url-pattern>
	</servlet-mapping>
 
</web-app>
 